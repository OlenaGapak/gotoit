{
  "name": "go-to-it",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "homepage": "./",
  "devDependencies": {
    "bootstrap-loader": "^1.3.2",
    "css-loader": "^0.26.1",
    "electron-builder": "^20.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^4.0.0",
    "react-scripts": "0.7.0",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "7zip-bin-linux": "^1.3.1",
    "app-builder-bin-linux": "^1.8.9"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-bootstrap": "^0.30.6",
    "react-bootstrap-slider": "^1.1.3",
    "react-chartjs-2": "^2.7.2",
    "react-dom": "^15.6.2",
    "react-intl": "^2.1.5",
    "react-modal": "^1.6.4",
    "react-motion": "^0.5.2",
    "react-portal": "^3.0.0",
    "react-select": "^1.0.0-rc.2",
    "react-toastr": "^2.8.2",
    "redux": "^3.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "desktop-linux": "electron-builder",
    "desktop-windows": "electron-builder --win --x64",
    "desktop-mac": "electron-builder --mac --x64"
  },
  "build": {
    "electronVersion": "1.6.15",
    "appId": "go-to-it",
    "extends": null,
    "directories": {
      "buildResources": "./build"
    },
    "files": [
      {
        "from": "./electron",
        "to": "./",
        "filter": [
          "index.js"
        ]
      },
      {
        "from": "./build",
        "to": "./"
      },
      {
        "from": "./",
        "to": "./",
        "filter": [
          "package.json"
        ]
      }
    ],
    "linux": {
      "target": "deb",
      "maintainer": "BlackDeckCrew"
    }
  }
}
