    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.14.0",
    "react-hot-loader": "^4.0.0",
    "resolve-url-loader": "^2.3.0",
    "speed-measure-webpack-plugin": "^1.2.2",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.56",
    "@babel/runtime": "7.0.0-beta.56",
    "animate.css": "^3.6.1",
    "autoprefixer": "^9.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-named-asset-import": "1.0.0-next.3e165448",
    "babel-preset-react-app": "4.0.0-next.3e165448",
    "bfj": "6.1.1",
    "bootstrap": "4.1.3",
    "cache-loader": "^1.2.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "chart.js": "^2.7.2",
    "circular-dependency-plugin": "^5.0.2",
    "css-loader": "1.0.0",
    "detect-port-alt": "^1.1.6",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-react-app": "3.0.0-next.3e165448",
    "eslint-loader": "2.1.0",
    "eslint-plugin-destructuring": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-lodash": "^2.7.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "7.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "html-webpack-plugin": "^4.0.0-alpha",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.4.2",
    "jquery": "^3.2.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.2",
    "node-sass-chokidar": "^1.3.3",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-slider": "^2.1.5",
    "react-chartjs-2": "^2.7.2",
    "react-dev-utils": "6.0.0-next.3e165448",
    "react-dom": "^16.4.1",
    "react-intl": "^2.1.5",
    "react-modal": "^3.5.1",
    "react-motion": "^0.5.2",
    "react-portal": "^3.0.0",
    "react-scroll-box": "^0.3.5",
    "react-select": "^1.0.0-rc.2",
    "react-toastr": "^2.8.2",
    "redux": "^4.0.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.21.0",
    "svgr": "2.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "thread-loader": "1.2.0",
    "toastr": "^2.1.4",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "4.16.4",
    "webpack-dev-server": "3.1.5",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "watch-sass-theme": "./node_modules/node-sass-chokidar/bin/node-sass-chokidar ./src/assets/styles/scss/theme -o ./src/assets/styles/ --watch  --source-map true --source-map-embed",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint:js": "eslint ./src/**/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:scss": "stylelint './src/**/*.scss'",
    "test": "node scripts/test.js --env=jsdom",
    "desktop-linux": "electron-builder",
    "desktop-windows": "electron-builder --win --x64",
    "desktop-mac": "electron-builder --mac --x64"
  },
  "build": {
    "electronVersion": "1.6.15",
    "appId": "go-to-it",
    "extends": null,
    "directories": {
      "buildResources": "./build"
    },
    "files": [
      {
        "from": "./electron",
        "to": "./",
        "filter": [
          "index.js"
        ]
      },
      {
        "from": "./build",
        "to": "./"
      },
      {
        "from": "./",
        "to": "./",
        "filter": [
          "package.json"
        ]
      }
    ],
    "linux": {
      "target": "deb",
      "maintainer": "BlackDeckCrew"
    }
  },
  "optionalDependencies": {
    "7zip-bin-linux": "^1.3.1",
    "app-builder-bin-linux": "^1.8.9"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
